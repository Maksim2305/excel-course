{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "semi": "off",
        "quotes": "off",
        "linebreak-style":"off",
        "require-jsdoc":"off",
        "eol-last":"off",
        "comma-dangle":"off",
        "object-curly-spacing": "off",
        "operator-linebreak": "off",
        "no-debugger":"off",
        "indent":"off",
        "max-len":"off",
        "prefer-const":"off",
        "no-unused-vars":"off"
    }
}